{
    "manifestVersion": 1,
    "id": "flutter",
    "version": "0.2.42.0",
    "name": "Flutter",
    "description": "Flutter extension for Azure DevOps",
    "publisher": "rajatanantharam-funda",
    "public": true,
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "icon.png"
    },
    "screenshots": [],
    "content": {
        "details": {
            "path": "README.md"
        },
        "license": {
            "path": "LICENSE"
        }
    },
    "links": {
        "support": {
            "uri": "https://github.com/rajatanantharam/vsts-flutter-tasks.git/issues"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/rajatanantharam/vsts-flutter-tasks.git"
    },
    "tags": [
        "flutter",
        "mobile",
        "ios",
        "android"
    ],
    "files": [
        {
            "path": "tasks/install"
        },
        {
            "path": "tasks/build"
        },
        {
            "path": "tasks/test"
        },
        {
            "path": "tasks/command"
        },
        {
            "path": "images",
            "addressable": true
        }
    ],
    "contributions": [
        {
            "id": "flutter-install",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "tasks/install"
            }
        },
        {
            "id": "flutter-build",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "tasks/build"
            }
        },
        {
            "id": "flutter-test",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "tasks/test"
            }
        },
        {
            "id": "flutter-command",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "tasks/command"
            }
        }
    ]
}